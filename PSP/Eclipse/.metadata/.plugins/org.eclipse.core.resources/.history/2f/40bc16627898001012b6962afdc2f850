package prueba;

import java.util.Scanner;

public class Suma {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		Suma(1, 5);
	}

	public static void Suma(int n1, int n2) {
		int resultado = 0;

		for (int i = 0; i < n2; i++) {
			resultado = resultado + 1;
		}
		System.out.println(resultado);
	}
}
